{
  "version": 2,
  "name": "DevUx",
  "alias": ["devux"],
  "regions": ["bru1"],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "build": {
    "env": {
      "SPACE_ID": "@contentful-space-id",
      "ACCESS_TOKEN": "@contentful-delivery-access-token",
      "ANALYTICS_ID": "@google-analytics-id"
    }
  }
}
