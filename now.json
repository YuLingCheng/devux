{
  "version": 2,
  "name": "DevUx",
  "alias": ["devux"],
  "regions": ["bru1"],
  "builds": [
    { "src": "package.json", "use": "@now/static-build", "config": { "distDir": "build" } }
  ],
  "routes": [
    {"src": "^/static/(.*)", "dest": "/static/$1"},
    {"src": "/favicon.png", "dest": "/favicon.ico"},
    {"src": ".*", "dest": "/index.html"}
  ]
}